{
  "manifest_version": 3,
  "name": "ReluExtension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "action": {
    "default_title": "Will Smith",
    "default_popup": "popup.html"
  },
  "permissions":["notifications"] 
}
